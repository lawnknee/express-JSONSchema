{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://localhost:3000/shipIt.schema.json",
  "title": "Product",
  "description": "Details about a product",
  "type": "object",
  "properties": {
    "productId": {
      "type": "integer",
      "minimum": 1000
    },
    "name": { "type": "string" },
    "addr": { "type": "string" },
    "zipcode": { "type": "string" }
  },
  "additionalProperties": false,
  "required": [
    "productId",
    "name",
    "addr",
    "zipcode"
  ]
}